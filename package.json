{
  "name": "@yozora/root",
  "version": "0.0.0",
  "description": "render md data in react",
  "author": {
    "name": "lemon-clown",
    "url": "https://github.com/lemon-clown/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemon-clown/yozora-react.git"
  },
  "homepage": "https://github.com/lemon-clown/yozora-react/#readme",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 12.16.0"
  },
  "scripts": {
    ":publish": "yarn format && lerna publish --no-private",
    "test": "lerna exec --concurrency 1 -- 'echo && pwd && yarn test'",
    "build": "lerna exec --concurrency 1 -- 'echo && pwd && yarn build'",
    "clean": "lerna clean --yes && yarn install",
    "rebuild": "yarn clean && yarn build",
    "format": "yarn find-inconsistent && yarn sort-imports && yarn lint:fix",
    "lint:fix": "lerna exec --concurrency 1 -- 'echo && pwd && yarn lint:fix'",
    "find-inconsistent": "barusu-find-inconsistent .",
    "sort-imports": "barusu-sort-imports ."
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
  },
  "devDependencies": {
    "@barusu/tool-find-inconsistent": "0.0.40",
    "@barusu/tool-sort-imports": "0.0.40",
    "@types/jest": "26.0.15",
    "@types/node": "^13.13.30",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "cross-env": "~7.0.2",
    "lerna": "3.22.1",
    "jest": "26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "~3.0.2",
    "styled-components": "^5.2.1",
    "ts-jest": "26.4.3",
    "ts-node": "~9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "~4.0.5"
  },
  "@barusu/tool-sort-imports": {
    "logLevel": "verbose",
    "pattern": [
      "{packages}/*/{src,__test__}/**/*.{ts,tsx}",
      "!**/{__test__}/cases",
      "!**/node_modules/"
    ],
    "moduleRanks": [
      {
        "regex": "^(react|vue|angular)(?:[\/\\-][\\w\\-.\/]*)?$",
        "rank": 1.1
      },
      {
        "regex": "^mocha|chai(?:[\/][\\w\\-.\/]*)?$",
        "rank": 1.2
      },
      {
        "regex": "^[a-zA-Z\\d][\\w\\-.]*",
        "rank": 1.3
      },
      {
        "regex": "^@[a-zA-Z\\d][\\w\\-.]*\\/[a-zA-Z\\d][\\w\\-.]*",
        "rank": 1.4
      },
      {
        "regex": "^@\\/[^\\n]*\\.(css|styl)$",
        "rank": 2.2
      },
      {
        "regex": "^@\\/",
        "rank": 2.1
      },
      {
        "regex": "^[.][\\/\\\\][^\\n]*\\.(css|styl)$",
        "rank": 3.4
      },
      {
        "regex": "^(?:\\/|[a-zA-Z]:)",
        "rank": 3.1
      },
      {
        "regex": "^[.]{2}[\\/\\\\][^\\n]*",
        "rank": 3.2
      },
      {
        "regex": "^[.][\\/\\\\][^\\n]*",
        "rank": 3.3
      }
    ],
    "indent": "  ",
    "quote": "'",
    "semicolon": false,
    "maxColumn": 80
  }
}
