{
  "name": "@yozora/root",
  "version": "0.0.0",
  "private": true,
  "description": "render md data in react",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/yozora-react.git"
  },
  "homepage": "https://github.com/guanghechen/yozora-react/#readme",
  "license": "MIT",
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    ":publish": "yarn test && lerna publish --no-private",
    "build": "lerna run build --concurrency 1",
    "clean": "lerna clean --yes && yarn install",
    "format": "run-s format:lint:fix format:prettier",
    "format:lint:fix": "eslint . --fix",
    "format:prettier": "prettier --write .",
    "prepublishOnly": "pinst --disable",
    "postinstall": "run-p postinstall:**",
    "postinstall:main": "yarn update:lock",
    "postinstall:dev": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "rebuild": "yarn clean && yarn build",
    "start": "lerna run start --parallel",
    "test": "lerna run --parallel test",
    "test:coverage": "lerna run test --parallel -- --coverage",
    "update:snapshot": "lerna run test --parallel -- -u",
    "update:changelog": "gitmoji-changelog update",
    "update:lock": "yarn-deduplicate"
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "postcss": "^7.0.35",
    "postcss-modules": "^3.2.2",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@guanghechen/eslint-config": "^1.4.1",
    "@guanghechen/eslint-config-jsx": "^1.4.1",
    "@guanghechen/eslint-config-ts": "^1.4.1",
    "@guanghechen/rollup-config-tsx": "^1.4.1",
    "@types/enzyme": "3.10.8",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.1",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/react-mathjax": "^1.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.4",
    "@wojtekmaj/enzyme-adapter-react-17": "0.4.1",
    "clsx": "^1.1.1",
    "cross-env": "7.0.3",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "fs-extra": "^9.1.0",
    "gitmoji-changelog": "2.2.0",
    "husky": "5.1.1",
    "is-ci": "3.0.0",
    "jest": "26.6.3",
    "jest-styled-components": "7.0.3",
    "lerna": "4.0.0",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-mathjax": "^1.0.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "3.0.2",
    "rollup": "2.47.0",
    "styled-components": "^5.2.1",
    "stylus": "^0.54.8",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4",
    "yarn-deduplicate": "3.1.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,eslintrc}": [
      "prettier --write"
    ]
  }
}
