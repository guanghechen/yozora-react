{
  "name": "@yozora/root",
  "version": "0.0.0",
  "description": "render md data in react",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/yozora-react.git"
  },
  "homepage": "https://github.com/guanghechen/yozora-react/#readme",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    ":publish": "yarn format && yarn test && lerna publish",
    "start": "lerna run --parallel start",
    "build": "lerna run --concurrency 1 build",
    "rebuild": "yarn clean && yarn build",
    "test": "lerna run --parallel test",
    "test:fix": "lerna exec --parallel 'yarn test -u'",
    "clean": "lerna clean --yes && yarn install",
    "format": "run-s format:lint:fix format:prettier",
    "format:lint:fix": "eslint . --fix",
    "format:prettier": "prettier --write .",
    "find-inconsistent": "barusu-find-inconsistent ."
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@barusu-react/rollup-config": "^0.0.19",
    "@barusu/tool-find-inconsistent": "^0.0.43",
    "@guanghechen/eslint-config": "1.0.10",
    "@guanghechen/eslint-config-jsx": "1.0.10",
    "@guanghechen/eslint-config-ts": "1.0.10",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "26.0.19",
    "@types/node": "^13.13.30",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-mathjax": "^1.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "cross-env": "~7.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "jest-styled-components": "7.0.3",
    "lerna": "^4.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "~3.0.2",
    "rollup": "2.35.1",
    "styled-components": "^5.2.1",
    "ts-jest": "26.4.4",
    "ts-node": "~9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "~4.1.3"
  }
}
