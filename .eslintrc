{
  "root": true,
  "extends": [
    "@guanghechen",
    "@guanghechen/jsx",
    "plugin:jest/recommended",
    "prettier"
  ],
  "plugins": ["import"],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "no-prototype-builtins": 0
  },
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx"],
      "extends": ["@guanghechen/jsx", "@guanghechen/ts", "prettier"],
      "rules": {
        "no-prototype-builtins": 0
      }
    },
    {
      "files": ["**/__test__/*.spec.ts", "**/__test__/*.spec.tsx"],
      "extends": [
        "@guanghechen/jsx",
        "@guanghechen/ts",
        "plugin:jest/recommended",
        "prettier"
      ],
      "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-prototype-builtins": 0
      }
    }
  ]
}
