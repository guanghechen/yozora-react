{
  "version": "0.0.7",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "graphType": "all",
      "verifyAccess": false,
      "ignoreChanges": [],
      "message": ":bookmark:  chore(release): publish %s",
